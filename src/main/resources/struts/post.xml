<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="post" extends="json-default">
        <action name="postAction_findAll" class="com.lanou.crm.department.action.DepartmentAction" method="list">
            <result name="success">/WEB-INF/pages/post/listPost.jsp</result>
        </action>

        <action name="postAction_addOrEdit" class="com.lanou.crm.post.action.PostAction" method="add">
            <result name="success">/WEB-INF/pages/post/addOrEditPost.jsp</result>
        </action>

    </package>
</struts>